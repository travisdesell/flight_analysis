#! /usr/bin/gnuplot

reset
set key right top box opaque font "Times New Roman, 48" samplen 2 width -6
set terminal png font "Times New Roman,64" size 3000, 1500 crop
set title "MSL Altitude Estimation"
set datafile missing "-"

set style data lines
set key right box
set ylabel "Normalized MSL Altitude"
set xlabel "Timestep"
set output altitude_output_file

plot input_file every 1 u 8 ti 'Predicted MSL Altitude', \
     input_file every 1 u 9 ti 'Actual MSL Altitude'

